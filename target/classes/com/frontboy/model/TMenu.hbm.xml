<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.frontboy.model.TMenu" table="t_menu" schema="integrity">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="component" column="component"/>
        <property name="pid" column="pid"/>
        <property name="icon" column="icon"/>
        <property name="path" column="path"/>
        <property name="componentName" column="component_name"/>
        <property name="createTime" column="create_time"/>
    </class>
</hibernate-mapping>